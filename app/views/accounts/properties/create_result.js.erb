$('.form-container').fadeOut('fast', function () {
  var myblock = $("<%= escape_javascript(@property.errors.empty? ? render('created') : render('not_created'))%>").hide();
  $(this).replaceWith(myblock);
  $(".form-" + "<%= @property.errors.empty? ? 'container' : 'error' %>" ).fadeIn('fast');
  $('.back-button').returnOnClick("<%= properties_path %>");
});